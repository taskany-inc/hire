    "name": "@taskany/hire",
    "version": "0.27.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "build:styles": "npx sass --no-source-map ./src/styles/react-big-calendar/styles.scss ./react-big-calendar.css",
        "start": "next start",
        "lint": "next lint && eslint --ext .js,.ts,.tsx .",
        "db:generate": "prisma migrate dev --name $1",
        "db:migrate": "prisma migrate deploy",
        "db:seed": "npx prisma db seed",
        "dev:db:init": "npx prisma migrate dev --preview-feature && npx prisma db seed",
        "generate:i18n": "generate-i18n --func=tr --langs=en,ru --path='./src/{components,controllers,pages,hooks,utils,plugins,i18n,schema,backend,contexts,modules}/**/*' --getLang='./src/utils/getLang'",
        "test": "echo 'No tests yet'",
        "prepare": "husky install",
        "type-check": "npx tsc"
    },
    "prisma": {
        "seed": "ts-node --files --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
    },
